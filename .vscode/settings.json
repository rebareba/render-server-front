{
  "eslint.enable": true,
  "eslint.options": {
      "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx",
          ".json"
      ]
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 5000,
}
